	<declaration>// Place global declarations here.
int direction;
int locX;
int locY;
int total_steps;
int visited[3][3];
clock x;

void initialize(){
    direction = 0;
    locX = 0;
    locY = 0;
    visited[0][0] = 1;
    total_steps = 0;

}

void turn(){
    direction++;
    direction = direction % 4;
    total_steps++;

}

bool isAtStart() {
    return locX == 0 &amp;&amp; locY == 0;
}

void move(){ 
    if (direction == 0){
        locY++;
    } else if (direction == 1){
        locX++;
    } else if (direction == 2){
        locY--;
    } else if (direction == 3){
        locX--;
    }
    visited[locX][locY]++;
    total_steps++;

}

bool allVisited(){
    return (visited[0][0] != 0) &amp;&amp; (visited[0][1] != 0) &amp;&amp; (visited[0][2] != 0) &amp;&amp; (visited[1][0] != 0) &amp;&amp; (visited[1][1] != 0) &amp;&amp; (visited[1][2] != 0) &amp;&amp; (visited[2][0] != 0) &amp;&amp; (visited[2][1] != 0) &amp;&amp; (visited[2][2] != 0);
}</declaration>
