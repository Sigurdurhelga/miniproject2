	<declaration>// Place global declarations here.
int direction = 0;
int locX = 0;
int locY = 0;

bool visited[3][3];

void turn(){
    direction++;
    direction = direction % 4;
}

void move(){
    visited[locX][locY] = true;    
    if (direction == 0){
        locY++;
    } else if (direction == 1){
        locX++;
    } else if (direction == 2){
        locY--;
    } else if (direction == 3){
        locX--;
    }
}</declaration>
